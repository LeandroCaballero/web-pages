{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "types": ["cypress", "node"],
      "module": "ESNext",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "moduleResolution": "node",
      "isolatedModules": true,
      "noEmit": true
    },
    "include": [
      "../node_modules/cypress",
      "./**/*.ts",
      "./**/*.d.ts"
    ]
  }
  